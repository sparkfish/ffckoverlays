{
    "manifest_version": 2,
    "name": "Overlay Remover",
    "description": "Right-click any overlay to remove it. ",
    "version": "1.0",
    "homepage_url": "https://github.com/sparkfish/overlay-remover",
	"icons": {
		"48": "icons/ext-48.png"
	},

    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "permissions": [
        "activeTab",
        "menus"
    ]
}
